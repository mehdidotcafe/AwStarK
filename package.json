{
  "name": "awstark",
  "version": "0.0.1",
  "description": "node.js typescript (AWS serverless) starter kit",
  "author": "mehdidotcafe",
  "private": false,
  "license": "GPL-3.0-or-later",
  "scripts": {
    "test": "jest",
    "lint": "eslint '*.ts' --quiet --fix",
    "compile": "tsc",
    "start": "rm -r .aws-sam & concurrently --names \"tsc,sam\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn start:tsc\" \"yarn start:sam\"",
    "start:tsc": "tsc -w",
    "start:sam": "(sam local start-api 2>&1 | tr '\\r' '\\n')"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "concurrently": "^7.6.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
